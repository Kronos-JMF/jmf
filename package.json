{
  "name": "jmf",
  "version": "0.0.0-semantic-release",
  "description": "JavaScript Modelling Framework. Base module",
  "keywords": [
    "JMF"
  ],
  "main": "index.js",
  "scripts": {
    "main": "lib/index.js",
    "debug": "npm run compile && mocha --debug-brk tests/*Test.js --compilers js:babel-register",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha tests/*Test.js --compilers js:babel-register",
    "doc": "jsdoc src/*.js",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-JMF/jmf.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-JMF/jmf/issues"
  },
  "homepage": "https://github.com/Kronos-JMF/jmf#readme",
  "engines": {
    "node": ">=5"
  },
  "contributors": [{
    "name": "Torsten Link",
    "email": "torstenlink@gmx.de"
  }, {
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.1.6",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.0",
    "semantic-release": "^4.3.5",
    "assert": "^1.4.1",
    "babel-cli": "6.9.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "^6.11.6"
  }
}
